59	INTO salariedAccount values(accountno.nextval,:h_minbalance,4.5);
61	INTO account values(accountno.currval,'savings',h_balance,:h_CustID);
76	INTO NonsalariedAccount values(accountno.nextval,:h_minbalance,4.5);
78	INTO account values(accountno.currval,'savings',:h_balance,:h_CustID);
90	INTO currentaccount values(accountno.nextval,:h_balance);
92	INTO account values(accountno.currval,'Current',:h_balance,:h_CustID);
121	INTO fixedaccount values(accountno.nextval,:h_depositAmount,:h_depositYears,:h_maturityAmount,sysdate);
123	INTO account values(accountno.currval,'FixedDeposit',:h_depositAmount,:h_CustID);
