CREATE TABLE rate (
	rate FLOAT,
	time NUMBER
);
CREATE TABLE ACCOUNT (
	accountno NUMBER NOT NULL,
	type VARCHAR,
	CUSTOMERID NUMBER,
	BALANCE FLOAT,
	status NUMBER,
	PRIMARY KEY (accountno)
);
CREATE TABLE atm (
	atmID NUMBER NOT NULL,
	location VARCHAR,
	balance FLOAT,
	region VARCHAR,
	thresholdValue FLOAT,
	status NUMBER,
	PRIMARY KEY (atmID)
);
CREATE TABLE BILL (
	BIILID NUMBER NOT NULL,
	CUSTOMERID NUMBER,
	type VARCHAR,
	BILLAMOUNT FLOAT,
	date DATE,
	REFID NUMBER,
	PRIMARY KEY (BIILID)
);
CREATE TABLE branchAccount (
	accountNo  NUMBER NOT NULL,
	branchid NUMBER,
	thresholdValue FLOAT,
	balance FLOAT,
	status NUMBER,
	PRIMARY KEY (accountNo )
);
CREATE TABLE Branch (
	branchid NUMBER NOT NULL,
	location VARCHAR,
	region VARCHAR,
	accountno NUMBER,
	branchmanager NUMBER,
	status NUMBER,
	PRIMARY KEY (branchid)
);

CREATE TABLE CUSTOMER (
	customerID NUMBER NOT NULL,
	NAME VARCHAR,
	DOB VARCHAR,
	GENDER CHAR,
	ADDRESS VARCHAR,
	status NUMBER,
	ATMSTATUS VARCHAR,
	PRIMARY KEY (customerID)
);

CREATE TABLE employee (
	employeeid NUMBER NOT NULL,
	ENAME VARCHAR,
	EGENDER CHAR,
	EADDRESS VARCHAR,
	EDESIGNATION VARCHAR,
	ESTATUS NUMBER,
	DOB DATE,
	PRIMARY KEY (employeeid)
);
CREATE TABLE fixedaccount (
	accountno NUMBER NOT NULL,
	DEPOSITAMT FLOAT,
	years NUMBER,
	macturitybalance FLOAT,
	CURRENTDATE DATE,
	PRIMARY KEY (accountno)
);
CREATE TABLE LOGIN (
	USERNAME NUMBER NOT NULL,
	PASSWORD VARCHAR,
	SEQANS1 VARCHAR,
	SEQANS2 VARCHAR,
	PRIMARY KEY (USERNAME)
);
CREATE TABLE REQUEST (
	requestid NUMBER NOT NULL,
	accountno NUMBER,
	type VARCHAR,
	reqdate DATE,
	PRIMARY KEY (requestid)
);
CREATE TABLE TRANSACTION (
	transactionID NUMBER NOT NULL,
	accountno NUMBER,
	transactiondate DATE,
	type VARCHAR,
	amount FLOAT,
	PRIMARY KEY (transactionID)
);

CREATE SEQUENCE transactionid
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE requestid
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE accountno
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE employeeid
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE SEQ_CUSTOMERID
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE branchid
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE accountno
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE ATM_ID
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE BILLREFID
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
CREATE SEQUENCE AccountNumber
  MINVALUE 1
  START WITH 1
  INCREMENT BY 1
  CACHE 20;
  
